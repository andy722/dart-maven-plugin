<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.google.dart</groupId>
  <artifactId>dart-maven-plugin</artifactId>
  <version>1.0.1</version>
  <packaging>maven-plugin</packaging>
  <name>Dart Maven Plugin</name>
  <description>Maven Plugin to manage dart SDK operations</description>
  <inceptionYear>2012</inceptionYear>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>dzwicker</id>
      <name>Daniel Zwicker</name>
      <email>email@daniel-zwicker.de</email>
      <url>www.daniel-zwicker.de</url>
      <timezone>+1</timezone>
    </developer>
  </developers>
  <prerequisites>
    <maven>3.0.4</maven>
  </prerequisites>
  <scm>
    <connection>scm:git:https://github.com/dzwicker/dart-maven-plugin.git</connection>
    <developerConnection>scm:git:https://github.com/dzwicker/dart-maven-plugin.git</developerConnection>
    <url>https://github.com/dzwicker/dart-maven-plugin</url>
  </scm>
  <distributionManagement>
    <repository>
      <id>in2ex-releases</id>
      <name>In2Experience Repository</name>
      <url>https://dev.in2experience.com/nexus/content/repositories/public-release/</url>
    </repository>
    <snapshotRepository>
      <id>in2ex-snapshots</id>
      <name>In2Experience Snapshot Repository</name>
      <url>http://dev.in2experience.com/nexus/content/repositories/snapshots/</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <settings.JAVA_HOME_7>/Library/Java/JavaVirtualMachines/jdk1.7.0_09.jdk/Contents/Home</settings.JAVA_HOME_7>
    <mavenPluginPluginVersion>3.1</mavenPluginPluginVersion>
    <settings.INVEX_HOME>/Users/daniel/In2Experience/invex</settings.INVEX_HOME>
    <mavenVersion>3.0.4</mavenVersion>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <settings.JBOSS_7_1_HOME>/Users/daniel/In2Experience/jboss-as-7.1.1.Final</settings.JBOSS_7_1_HOME>
  </properties>
